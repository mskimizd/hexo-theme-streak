<footer class="footer">
  <nav class="blog-nav">
    <ul class="menu">
      <% for (name in theme.menu) { %>
        <li class="menu-item">
          <a href="<%- url_for(theme.menu[name]) %>" class="menu-item-link"><%- __(name) %></a>
        </li>
      <% } %>
    </ul>
  </nav>  
  <div class="credit">
      Powered by 
      <a href="https://hexo.io/">Hexo</a> 
      and Theme by 
      <a href="https://github.com/mskimizd/hexo-theme-streak">Streak</a> 
  </div>
  <div class="copright">
    &copy; <%= date(new Date(), 'YYYY') %>
    <span class="author">
      <%= config.author || '' %>
    </span>
  </div>
</footer>